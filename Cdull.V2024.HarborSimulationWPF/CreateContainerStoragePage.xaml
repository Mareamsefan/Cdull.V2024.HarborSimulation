<Page x:Class="Cdull.V2024.HarborSimulationWPF.CreateContainerStoragePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="CreateContainerStoragePage" Height="350" Width="500">

    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Label Content="Container Storage Name:"/>
            <TextBox x:Name="StorageNameTextBox"/>
            <Label Content="Start Index:"/>
            <TextBox x:Name="StartIndexTextBox"/>
            <Label Content="End Index:"/>
            <TextBox x:Name="EndIndexTextBox"/>
            <Button Content="Create Container Storage" Click="CreateContainerStorage"/>
        </StackPanel>

    </Grid>
</Page>
