    <RepositoryUrl>https://github.com/Mareamsefan/Cdull.V2024.HarborSimulation</RepositoryUrl>
    <PackageProjectUrl>https://github.com/Mareamsefan/Cdull.V2024.HarborSimulation</PackageProjectUrl>
    <RepositoryType>git.</RepositoryType>
    <NeutralLanguage>en</NeutralLanguage>
    <PackAsTool>False</PackAsTool>
    <PackageIcon>CDULL.png</PackageIcon>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\..\Downloads\CDULL.png" Link="CDULL.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
